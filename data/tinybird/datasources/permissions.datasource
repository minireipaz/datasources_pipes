DATASOURCE permissions
ENGINE "MergeTree"
ENGINE_SORTING_KEY "id, role_id"

SCHEMA >
    `id` String `json:$.id`,
    `role_id` String `json:$.role_id`,
    `subject` String `json:$.subject`,
    `action` String `json:$.action`,
    `conditions` String `json:$.conditions`
